{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "NUDGE_LOGO.png",
      "use": "@vercel/static"
    },
    {
      "src": "api/chat.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "api/cuet-data.json",
          "api/nirf_rankings_2025.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/NUDGE_LOGO.png",
      "dest": "/NUDGE_LOGO.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}